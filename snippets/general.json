	"-> Console WriteLine": {
		"prefix": "cw",
		"body": [
			"Console.${1|WriteLine,Write|}($0);"
		],
		"description": "Console WriteLine();"
	},
	"-> WriteLine Interpolation": {
		"prefix": "cwi",
		"body": [
			"Console.${1|WriteLine,Write|}($\"${0:Text}\");"
